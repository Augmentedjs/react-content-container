{
  "name": "react-content-container",
  "version": "1.0.0",
  "description": "A Presentation component that wraps third-party content for use in react.js",
  "main": "index.tsx",
  "scripts": {
    "start": "rm -rf dist && NODE_ENV=development node --stack-size=2048 ./node_modules/webpack/bin/webpack serve --mode development --progress",
    "dev": "rm -rf dist && NODE_ENV=development node --stack-size=2048 ./node_modules/webpack/bin/webpack --mode development --progress",
    "build": "rm -rf dist && NODE_ENV=production node --stack-size=2048 ./node_modules/webpack/bin/webpack --mode production",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install --verbose",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "keywords": [
    "react",
    "typescript",
    "content",
    "integration"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/react-content-container.git"
  },
  "author": "Bob Warren <jedi.master@augmentedjs.com>",
  "license": "Apache-2.0",
  "engines": {
    "npm": ">= 9.0.0",
    "node": ">= 18"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "babel-loader": "^9.1.3",
    "babel-plugin-remove-template-literals-whitespace": "^1.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.1",
    "express-cache-headers": "^0.1.4",
    "favicons-webpack-plugin": "^6.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.7",
    "node-sass": "^9.0.0",
    "sass": "^1.70.0",
    "sass-loader": "^14.0.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/react-content-container/issues"
  },
  "homepage": "https://github.com/Augmentedjs/react-content-container"
}
